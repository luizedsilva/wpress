<%-include('../../partials/header.ejs'); %>
<%-include('../../partials/navbar.ejs'); %>

<div class="container">
  <hr />
  <h3>Usuários</h3>
  <hr />
  <a href="/admin/users/create" class="btn btn-success">Cadastrar usuário</a>
  <hr />
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Id</th>
        <th>email</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user=> { %>
      <tr>
        <th><%= user.id %></th>
        <th><%= user.email %></th>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<%- include('../../partials/footer.ejs'); %>
